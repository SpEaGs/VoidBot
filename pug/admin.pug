//link(rel='stylesheet', type='text/css', href='index.css')
style.
    @import url("https://fonts.googleapis.com/css?family=Cousine");

    @font-face {
        font-family: "Whitney A";
        font-style: normal;
        font-weight: 400;
        src:url("data:application/x-font-woff;base64,")
    }

    @font-face {
        font-family: "Cousine", monospace;
        font-style: normal;
        font-weight: 400;
    }

    :root {
        --titlebar-color: rgb(20, 20, 25);
        --toolbar-color: rgb(30, 30, 40);
        --toolbar-hover-color: rgb(40, 40, 48);
        --toolbar-selected-color: rgb(50, 50, 60);
        --navbar-hover-color: rgb(58, 58, 65);
        --main-bg-color: rgb(70, 70, 80);
        --accent-color: rgb(35, 62, 216);
        --accent-color-hover: rgb(70, 93, 225);
        --track-color: rgb(173, 173, 173);
        --track-color-hover: rgb(211, 211, 211);
    }

    ::-webkit-scrollbar {
        width: 8px;
    }
    ::-webkit-scrollbar-track {
        background: var(--track-color);
    }
    ::-webkit-scrollbar-track:hover {
        background: var(--track-color-hover);
    }
    ::-webkit-scrollbar-thumb {
        background: var(--accent-color);
    }
    ::-webkit-scrollbar-thumb:hover {
        background: var(--accent-color-hover);
    }

    html, body {
        width: 100%;
        height: 100%;
        margin: 0;
        background-color: var(--main-bg-color);
        color: white;
        font-family: "Whitney A";
    }

    p {
        margin:0;
        padding:0;
    }

    .titleButtonContainer {
        height: 2em;
        width: 6em;
        position: fixed;
        top: 0px;
        left: calc(100% - 6em);
        display: flex;
        flex-flow: row-reverse nowrap;
        justify-content: space-around;
        align-items: center;
    }
    .titleButton {
        height: calc(2em - 10px);
        width: calc(2em - 10px);
        -webkit-app-region: no-drag;
        transition: ease 0.3s all;
        cursor: pointer;
    }

    .contentBody {
        display: flex;
        height: calc(100% - 2em);
        width: 100%;
        position:fixed;
        top: 2em;
    }
    .flex {
        display: flex;
        flex-wrap: wrap;
        justify-content: space-around;
        align-content: flex-start;
    }
    .hidden {
        display: none;
    }

    .navbar {
        height: 2em;
        width: 25%;
        min-width: 180px;
        position: fixed;
        top: 2em;
        left: 0px;
        background-color: var(--toolbar-color);
    }
    .navbarContainer {
        display: flex;
        flex-flow: row nowrap;
        justify-content: space-between;
        align-items: center;
        width: 25%;
        min-width: 180px;
        height: 2em;
    }
    .navbarButton {
        height: 2em;
        width: 50%;
        cursor:pointer;
        transition: ease 0.3s all;
        background-color: var(--toolbar-color);
    }
    .navbarButton:hover {
        background-color: var(--toolbar-hover-color);
    }
    .navbarButtonText {
        position: relative;
        top: 0.25em;
        text-align: center;
        cursor:inherit;
    }
    .navbarButtonSelected {
        background-color: var(--toolbar-selected-color);
    }

    .navbarMenuContainer {
        height: calc(100% - 4em);
        width: 25%;
        min-width: 180px;
        position: fixed;
        top: 4em;
        left: 0px;
        flex-flow: column nowrap;
        background-color: var(--toolbar-selected-color);
        overflow-y: auto;
        overflow-x:hidden;
    }
    .navbarMenuButton {
        height: 1.5em;
        width: 100%;
        cursor: pointer;
    }
    .navbarMenuButton:hover {
        background-color: var(--navbar-hover-color);
    }
    .navbarMenuButtonText {
        position: relative;
        top: 0.125em;
        left: 0.5em;
        text-align: left;
        cursor: inherit;
    }
    .navbarMenuButtonSelected {
        background-color: var(--main-bg-color);
    }

    .mainContentContainer {
        height: calc(100% - 2em);
        width: 75%;
        position: fixed;
        left: 25%;
        top: 2em;
        overflow-y: auto;
        cursor: default;
    }

    .mainContentItem {
        height: 3.5em;
        width: 100%;
        top: 0px;
        margin: 0px;
        background-color:inherit;
        transition: ease 0.3s all;
        cursor: pointer;
    }
    .mainContentItem:hover {
        background-color: var(--navbar-hover-color);
    }
    .navbarSubMenuContentContainerFlex{
        height: calc(100% - 2em);
        width: 75%;
        position: fixed;
        left: 25%;
        top: 2em;
        overflow-y: auto;
        cursor: default;
        display: flex;
        flex-wrap: wrap;
        justify-content: space-around;
        align-content: flex-start;
    }
    .mainContentSettingItem {
        background-color: var(--toolbar-selected-color);
        position: relative;
        width: 300px;
        height: 225px;
        margin: 1em;
    }
    .mainContentSettingItemText {
        position: relative;
        padding-top: 1em;
        padding-left: 1em;
        padding-right: 1em;
        font-size: 2em;
        text-wrap: normal;
        text-align: center;
    }
    .maincontentSettingItemSubtext {
        font-size: 1em;
        text-wrap: normal;
        text-align: center;
    }
    .mainContentItemText {
        position:relative;
        left: 3.5em;
        width: calc(100% - 3.5em);
        top: 0.6em;
        -webkit-user-select:none;
        cursor:pointer;
    }
    .dropdownButton {
        padding: 10px;
        width: 100%;
        height: 100%;
        color: white;
        font-family: "Whitney A";
        font-size: 1.5em;
        text-align: center;
        background-color: var(--toolbar-color);
        transition: ease 0.3s all;
        border: none;
        cursor: pointer;
    }
    .dropdownButton:hover {
        background-color: var(--toolbar-hover-color);
    }
    .dropdown {
        width: calc(100% - 4em);
        position: relative;
        padding: 2em;
        display: inline-block;
    }
    .dropdownContent {
        position: absolute;
        max-height: 15em;
        overflow-y: auto;
        background-color: var(--navbar-hover-color);
        min-width: 50%;
        z-index: 1;
    }
    .dropdownContent a {
        display: block;
        color: white;
        text-decoration: none;
        padding: 0.25em;
        font-size:1em;
    }
    .dropdownContent a:hover {
        background-color: var(--toolbar-selected-color)
    }

    .rangeSliderContainer {
        padding: 1.5em;
        padding-top: 3.5em;
        width: 100%;
        height: 1em;
    }
    .slider {
        -webkit-appearance: none;
        appearance: none;
        width: calc(100% - 4em);
        height: 25px;
        background: var(--track-color);
        outline: none;
        opacity: 0.7;
        -webkit-transition: .2s;
        transition: opacity .2s;
    }
    .slider:hover {
        opacity: 1;
    }
    .slider::-webkit-slider-thumb {
        -webkit-appearance: none;
        appearance: none;
        width: 25px; /* Set a specific slider handle width */
        height: 25px; /* Slider handle height */
        background: var(--accent-color); /* Green background */
        cursor: pointer; /* Cursor on hover */
    }
    .slider::-moz-range-thumb {
        width: 25px; /* Set a specific slider handle width */
        height: 25px; /* Slider handle height */
        background: var(--accent-color); /* Green background */
        cursor: pointer; /* Cursor on hover */
    }

    .toggleSwitchContainer {
        position: relative;
        display: inline-block;
        width: 236px;
        height: 49px;
        left: calc(50% - (236px/2));
        top: 33px;
    }
    .toggleSwitchContainer input {
        opacity: 0;
        width: 0;
        height: 0;
    }
    .toggleSlider {
        position: absolute;
        cursor: pointer;
        top: 0;
        left: 0;
        bottom: 0;
        right: 0;
        background-color: var(--toolbar-color);
        -webkit-transition: 0.3s;
        transition: 0.3s;
        border-radius: 24px;
    }
    .toggleSlider:before {
        position: absolute;
        content: "";
        height: 41px;
        width: 114px;
        left: 4px;
        bottom: 4px;
        background-color: white;
        -webkit-transition: 0.3s;
        transition: 0.3s;
        border-radius: 24px;
    }

    input:checked + .toggleSlider {
        background-color: var(--accent-color);
    }
    input:focus + .toggleSlider {
        box-shadow: 0, 0, 1px, var(--accent-color);
    }
    input:checked + .toggleSlider:before {
        -webkit-transform: translateX(114px);
        -ms-transform: translateX(114px);
        transform: translateX(114px);
    }

    #titleBar {
        width: 100%;
        height: 2em;
        margin: 0;
        position: fixed;
        top: 0px;
        background-color: var(--titlebar-color);
        -webkit-user-select: none;
        -webkit-app-region: drag;
    }
    #titleText {
        position: relative;
        margin-left: 1em;
        top: 0.5em;
    }
    #titleButtonClose {
        background: url('assets/titleButton/titleButtonClose.png');
    }
    #titleButtonClose:hover {
        background: url('assets/titleButton/titleButtonCloseDark.png');
    }
    #titleButtonMaximize {
        background: url('assets/titleButton/titleButtonMax.png');
    }
    #titleButtonMaximize:hover {
        background: url('assets/titleButton/titleButtonMaxDark.png');
    }
    #titleButtonMinimize {
        background: url('assets/titleButton/titleButtonMin.png');
    }
    #titleButtonMinimize:hover {
        background: url('assets/titleButton/titleButtonMinDark.png');
    }

    #mainContentItemSTDOUT {
        font-family: "Cousine";
        height: auto;
        width: auto;
        margin-top: 0.25em;
        margin-left: 0.5em;
    }
    #mainContentItemSTDOUT:hover {
        background: var(--main-bg-color);
        cursor: default;
    }

#titleBar
    strong#titleText VoidBot v 
        script#version(ver=appVersion).
            document.write(document.getElementById('version').getAttribute('ver'));
.contentBody#windowContent
    include navbar.pug